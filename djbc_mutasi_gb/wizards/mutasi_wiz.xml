<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
	<record id="mutasiwizardgb_form" model="ir.ui.view">
		<field name="name">Mutasi GB Wizard Form</field>
		<field name="model">djbc.mutasiwizardgb</field>
		<field name="arch" type="xml">
			<form string="Laporan Mutasi">
				<header>
					Laporan Mutasi
				</header>
				<group>
					<field name="date_start"/>
					<field name="date_end"/>
					<field name="djbc_category_id" options="{'no_create': True}"/>
					<field name="kategori" />
				</group>
				<footer>
					<button name='generate_laporan' string='Submit' type='object' class='btn-primary'/>
					<button name='generate_laporan_xls' string='Print Xls' type='object' class='btn-primary'/>
					<button string='Cancel' class='btn-secondary' special='cancel'/>
				</footer>
			</form>
		</field>
	</record>

	<record id="mutasiwizardgb_action" model="ir.actions.act_window">
		<field name="name">Masukan Periode dan Category</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.mutasiwizardgb</field>
		<field name="view_mode">form</field>
		<field name="view_id" ref='mutasiwizardgb_form'/>
		<field name="target">new</field>
	</record>
	</data>
</odoo>