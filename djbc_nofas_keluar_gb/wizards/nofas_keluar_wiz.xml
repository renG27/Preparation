<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="nofas_keluar_gb_wizard_form" model="ir.ui.view">
        <field name="name">Laporan Pengeluaran</field>
        <field name="model">djbc.nofas.keluar.gb.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="date_start"/>
                    <field name="date_end"/>
                    <field name="tipe_dokumen_id"/>
                </group>
                <footer>
                    <button name='call_djbc_nofas_keluar_gb' string='Submit' type='object' class='btn-primary'/>
                    <button name='generate_laporan_xls' string='Print XLS' type='object' class='btn-primary'/>
                    <button string='Cancel' class='btn-secondary' special='cancel'/>
                </footer>
            </form>
        </field>
    </record>

    <record id="nofas_keluar_gb_wizard_action" model="ir.actions.act_window">
        <field name="name">Masukan Periode DJBC Documents</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">djbc.nofas.keluar.gb.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref='nofas_keluar_gb_wizard_form'/>
        <field name="target">new</field>
    </record>
</odoo>